#code
for _ in range(int(input())):
    n = int(input())
    l = list(map(int,input().split()))
    e = []
    o = []
    for i in l:
        if (i%2==0):
            e.append(i)
        else:
            o.append(i)
    e.sort()
    o.sort()
    o.reverse()
    for i in o:
        print(i,end=' ')
    for i in e:
        print(i,end=' ')
    print()
